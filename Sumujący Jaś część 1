#include <iostream>
#include <algorithm>
#include <string.h>

using namespace std;

char c[100];

char* pushing_string(char a[])//USUWA ZNAK
{
    for(int i = 0; i < strlen(a) - 1; i++)
    {
        a[i] = a[i + 1];
    }
    a[strlen(a) - 1] == '\0';

    return a;
}
char* longg(char a[], char b[])//SPRAWDZA DLUZSZY
{
    if(strlen(a) < strlen(b))return b;
    return a;
}
char* shortt(char a[], char b[])//SPRAWDZA KROTSZU
{
    if(strlen(a) < strlen(b))return a;
    return b;
}
char* add(char a[], char b[])//DODAJE I ZWRACA WYNIK
{
    strrev(a);
    strrev(b);

    int endd = 0;

    endd = strlen(b);

    int t = 0;
    int number = 0;
    char charr;

    for(int i = 0; i < endd; i++)
    {
        number = a[i] - '0' + b[i] - '0' + t;
        charr = (number % 10) + '0';
        t = number / 10;
        c[i] = charr;
    }

    if(strlen(a) != strlen(b))
    {
        for(int i = endd; i < strlen(a); i++)
        {
            c[i] = a[i];
            if(i == endd)c[i] += t;
        }
    }
    if(strlen(a) == strlen(b) && t != 0)
    {
        c[endd] = t + '0';
    }

    strrev(c);

    return c;
}

int main()
{
    char a[100];
    char b[100];

    cin >> a >> b;


    cout << add(longg(a, b), shortt(a, b));


    return 0;
}
